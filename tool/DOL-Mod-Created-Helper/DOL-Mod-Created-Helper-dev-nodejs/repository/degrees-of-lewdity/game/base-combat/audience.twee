			<<audiencecamera>>
			<br>
		<</if>>

		<<set $facepic to 0>>
		<<set $breastpic to 0>>
		<<set $bottompic to 0>>
		<<set $genitalpic to 0>>
		<<set $bodypic to 0>>

		<<if $audiencecamera1 isnot 0 and $face is "covered">>
			<span class="teal">At least your face is hidden.</span>
			<br>
		<</if>>

		<<audiencespeech>>
		/*<br> commented out due to excess space they caused*/
		<<if $rng - $enemyanger + (($enemyarousalmax / $enemyarousal) * 100) gte 0>>
