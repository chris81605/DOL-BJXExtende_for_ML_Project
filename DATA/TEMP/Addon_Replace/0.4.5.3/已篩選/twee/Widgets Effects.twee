		<<garousal>><<arousal _squidArousal "breasts">><<arousal _squidArousal "genitals">><<arousal _squidArousal "bottom">>
	<</if>>

	<<if !$worn.upper.type.includes("naked") and !waterproofCheck($worn.upper)>>
