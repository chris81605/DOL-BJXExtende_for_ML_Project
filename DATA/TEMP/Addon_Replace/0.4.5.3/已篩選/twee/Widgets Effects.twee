	<</if>>
	<<if $makeup.mascara and $makeup.mascara_running lt painToTearsLvl($pain) and !$makeup.mascara.includes("waterproof")>>
		<<set $makeup.mascara_running to painToTearsLvl($pain)>>
	<</if>>
<</widget>>

