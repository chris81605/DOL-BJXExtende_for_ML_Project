							<<if $noTraitDescription>>
								<<print _clothesTrait>>
							<<else>>
								<mouse class = "tooltip-small">
									<<print _clothesTrait>>
									<span class="black"><<print _clothesDesc>></span>
								</mouse>
							<</if>>
