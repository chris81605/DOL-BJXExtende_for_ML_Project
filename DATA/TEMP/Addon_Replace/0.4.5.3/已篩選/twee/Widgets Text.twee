	<<if $pronoun is "m">>
		<<set _text_output to _person + "拉开" + _his + "的裤裆拉链，">>
	<<else>>
		<<set _text_output to _person + "掀起" + _his + "的裙摆，">>
	<</if>>
	<<if $NPCList[0].penis isnot "none">>
		<<set _text_output to _text_output + "露出" + _his + "勃起的阴茎。">>
	<<else>>
		<<set _text_output to _text_output + "露出" + _his + "湿润的小穴。">>
	<</if>>
<</silently>>_text_output<</widget>>

