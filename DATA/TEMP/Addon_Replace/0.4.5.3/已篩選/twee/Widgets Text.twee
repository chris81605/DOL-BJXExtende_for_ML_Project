<<widget "generatePronouns">>
	<<if _args[0]>>
		<<set _args[0].pronouns to {}>>
		<<switch _args[0].pronoun>>
			<<case "m">>
				<<set _args[0].pronouns.he = "他">>
				<<set _args[0].pronouns.his = "他">>
				<<set _args[0].pronouns.hers = "他">>
				<<set _args[0].pronouns.him = "他">>
				<<set _args[0].pronouns.himself = "他自己">>
				<<set _args[0].pronouns.man = "男人">>
				<<set _args[0].pronouns.boy = "男孩">>
				<<set _args[0].pronouns.men = "男人们">>
			<<case "f">>
				<<set _args[0].pronouns.he = "她">>
				<<set _args[0].pronouns.his = "她">>
				<<set _args[0].pronouns.hers = "她">>
				<<set _args[0].pronouns.him = "她">>
				<<set _args[0].pronouns.himself = "她自己">>
				<<set _args[0].pronouns.man = "女人">>
				<<set _args[0].pronouns.boy = "女孩">>
				<<set _args[0].pronouns.men = "女人们">>
			<<case "i">>
				<<set _args[0].pronouns.he = "它">>
				<<set _args[0].pronouns.his = "它">>
				<<set _args[0].pronouns.hers = "它">>
				<<set _args[0].pronouns.him = "它">>
				<<set _args[0].pronouns.himself = "它自己">>
				<<set _args[0].pronouns.man = "男人">>
				<<set _args[0].pronouns.boy = "男孩">>
				<<set _args[0].pronouns.men = "男人们">>
			<<case "n">>
				<<set _args[0].pronouns.he = "一个">>
				<<set _args[0].pronouns.his = "那">>
				<<set _args[0].pronouns.hers = "它">>
				<<set _args[0].pronouns.him = "他们">>
				<<set _args[0].pronouns.himself = "他们自己">>
				<<set _args[0].pronouns.man = "男人">>
				<<set _args[0].pronouns.boy = "男孩">>
				<<set _args[0].pronouns.men = "男人们">>
			<<case "t">>
				<<set _args[0].pronouns.he = "他们">>
				<<set _args[0].pronouns.his = "他们">>
				<<set _args[0].pronouns.hers = "他们">>
				<<set _args[0].pronouns.him = "他们">>
				<<set _args[0].pronouns.himself = "他们自己">>
				<<set _args[0].pronouns.man = "男人">>
				<<set _args[0].pronouns.boy = "男孩">>
				<<set _args[0].pronouns.men = "男人们">>
		<</switch>>
	<</if>>
<</widget>>

<<widget "clearPronouns">>
