<<widget "he">><<silently>>
	<!-- Add the option to call for named NPCs individually -->
	<<if _args[0]>>
		<<set $pronoun to $NPCName[$NPCNameList.indexOf(_args[0])].pronoun>>
	<</if>>
	<<if $pronoun is "m">>
		<<set _text_output to "他">>
	<<elseif $pronoun is "f">>
		<<set _text_output to "她">>
	<<elseif $pronoun is "i">>
		<<set _text_output to "它">>
	<<elseif $pronoun is "n">>
		<<set _text_output to "其中一个">>
	<<elseif $pronoun is "t">>
		<<set _text_output to "他们">>
	<</if>>
<</silently>>_text_output<</widget>>

