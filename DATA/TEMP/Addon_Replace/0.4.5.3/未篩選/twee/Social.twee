					{ requiredValue: -100,	color: 'pink',		description: '亵渎', postText: '着你。'},
					{ requiredValue: -20,	color: 'purple',	description: '藐视', postText:  ''},
					{ requiredValue: -5,	color: '',			description: '警惕'},
					{ requiredValue: 5,		color: 'lblue',		description: '信任'},
