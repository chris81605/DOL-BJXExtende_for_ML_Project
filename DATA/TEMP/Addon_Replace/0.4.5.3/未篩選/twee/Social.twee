				<<set _farmHorseBoxConfig =		{ name: "马群",	description : '<<relation-text _farmHorseRepConfig>>'}>>
				<<set _farmPigBoxConfig =		{ name: "猪群",		description : '<<relation-text _farmPigRepConfig>>'}>>
