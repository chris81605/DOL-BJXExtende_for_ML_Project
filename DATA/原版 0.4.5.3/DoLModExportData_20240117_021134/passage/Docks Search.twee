		<br><br>
		<<earnFeat "Employee Benefits">>
		<<link [[拿走 (0:05)|Docks Gold]]>><<pass 5>><<crimeUp 3000 "thievery">><<set $blackmoney += 3000>><</link>><<crime "thievery">>
