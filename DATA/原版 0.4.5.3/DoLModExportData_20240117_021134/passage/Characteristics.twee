		<<set $_source to []>>
		<<set $_sourceText to "有什么东西">>
		<<set $_arousal to []>>
		<<set $_arousedText to "让你燥热难耐">>

		/* SourceText */
		<<if $wolfgirl gte 2 or $cat gte 2 or $cow gte 2 or $fox gte 2>>
			<<run $_source.push("内心的野兽")>>
		<</if>>
		<<if $earSlime.growth gt 50 and (($earSlime.focus is "pregnancy" and $_heatArousal) or ($earSlime.focus is "impregnation" and $_rutArousal))>>
			<<run $_source.push("耳中的史莱姆")>>
		<</if>>
		<<if $_source.length>>
			<<set $_sourceText to "你" + formatList($_source,"和", true,"、")>>
		<</if>>

		/* arousedText */
		<<if $_source.length and $_heatArousal and $_knowsTheyCanBecomePregnant>>
			<<run $_arousal.push("燥热")>>
		<</if>>
		<<if $_source.length and $_rutArousal>>
			<<run $_arousal.push("发情")>>
		<</if>>
		<<if $_arousal.length>>
			<<set $_arousedText to "让你" + formatList($_arousal, "和", true,"、")>>
		<</if>>
	<</if>>
	<</silently>><<if $_sourceText>><<print $_sourceText>><<print $_arousedText>>。<<if _args[0]>><br><br><</if>><</if>>
<</widget>>