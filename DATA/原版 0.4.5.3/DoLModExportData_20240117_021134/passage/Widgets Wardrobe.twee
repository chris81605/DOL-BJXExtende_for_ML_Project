					<</replace>>
				<<default>>
					<<replace `'#clothingBox-' + $_slot`>><<oldWardrobeList $_slot>><</replace>>
			<</switch>>
			<<replace '#wardrobeMinorOptions'>><<oldWardrobeMinorOptions>><</replace>>
		<<else>>
			<<replace '#oldWardrobeListDisplay'>><<oldWardrobeListDisplay>><</replace>>
