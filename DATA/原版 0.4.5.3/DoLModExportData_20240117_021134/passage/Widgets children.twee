					<</if>>
				<<else>>
					<<humanChildActivityNoToy _args[0]>>
				<</if>>
			<<case "dummySucking">>
				<<childHe>>正卖力吸吮<<childhis>>的橡胶奶嘴。
			<<case "clown">>
				<<childtoy $location "clown" "notext">>
				<<if _toyid>>
					<<childHe>>疑惑地盯着那个<<childtoy $location _toyid "lowercase">>。
					<<if $children[_args[0]].localVariables.event>>
