	<<elseif _guard_skill gte 400>>
		<span class="blue">C</span>
	<<elseif _guard_skill gte 200>>
		<span class="purple">D</span>
	<<else>>
		<span class="pink">F</span>
	<</if>>
<</widget>>

<<widget "farm_guard_pay">>
	<<if $farm.tower_guard_unpaid gte 1>>
