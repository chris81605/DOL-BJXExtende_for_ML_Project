	<</if>>
<</widget>>

<<widget "farm_stock_init">>
	<<set $farm.stock to {}>>
	<<set $farm.stock.truffles to 0>>
	<<set $farm.stock.eggs to 0>>
	<<set $farm.stock.milk to 0>>
<</widget>>

<<widget "farm_stock">>
	<<if _args[0]>>
		<<if _args[1] and _args[2]>>
			<<set $farm.stock[_args[0]] += random(_args[1], _args[2])>>
		<<elseif _args[1]>>
			<<set $farm.stock[_args[0]] += _args[1]>>
		<<else>>
			<<set $farm.stock[_args[0]] += 1>>
		<</if>>
	<</if>>
<</widget>>

<<widget "farm_ride_events">>
	<<rng 4>>
	<<if $rng is 4>>
		<<if $farm.aggro gte 80>>
			你听到马蹄声从后面靠近。雷米的两个打手<span class="red">跟在你后面。</span>
			<br><br>

			<<generate2>><<generate3>>
			<<link [[骑得更快|Farm Ride Aggro Faster]]>><</link>><<skill_difficulty `currentSkillValue("thighskill")` "腿部技巧" 400 1400>>
			<br>
			<<link [[对峙|Farm Ride Aggro Confront]]>><</link>>
			<br>
		<<elseif $farm.aggro gte 40>>
			你听到马蹄声从后面靠近。雷米的两个打手正跟在你后面。他们正逐渐追上你。
			<br><br>
			<<generate2>><<generate3>>
			<<link [[骑得更快|Farm Ride Faster]]>><</link>><<skill_difficulty `currentSkillValue("thighskill")` "腿部技巧" 1 1000>>
			<br>
			<<link [[对峙|Farm Ride Confront]]>><</link>>
			<br>
		<<else>>
			你听到马蹄从后面靠近。雷米的两个打手正跟着你，但他们保持着距离。<<gstress>><<stress 6>><<gaggro>><<farm_aggro 1>>
